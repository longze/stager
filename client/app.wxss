/**app.wxss**/
div,
section {
  display: block;
}

.black {
  color: aquamarine;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-star:before { content: "\e627"; }

.icon-local:before { content: "\e637"; }

.icon-search:before { content: "\e66f"; }

.icon-join:before { content: "\e614"; }

.icon-coffee:before { content: "\e664"; }

@font-face {
  font-family: 'iconfont';
  src: url(data:application/font-woff;charset=utf-8;base64,) format('woff');
  font-weight: normal;
  font-style: normal;
}

/* 底部版权 */
.bottom-right {
  width: 100%;
  text-align: center;
  font-size: 14px;
  line-height: 1.5em;
  color: #999999;
  margin: 15px 0 15px;
}

/* 底部按钮 */
.bottom-bar {
  position: fixed;
  bottom: 0;
  width: 100%;
  box-shadow: 0 -1px 4px 0 rgba(0, 0, 0, 0.20);
  text-align: center;
  font-size: 16px;
  line-height: 48px;
  color: #fff;
  background: #F75F48;
}
